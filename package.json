{
  "name": "rellat-web",
  "version": "0.1.3",
  "description": "The Simultaneous Collaboration Coding service to show people how to make code in real time.",
  "main": "src/index.js",
  "keywords": [
    "Collaboration",
    "crdt",
    "codemirror",
    "editor",
    "real-time",
    "multihack",
    "p2p",
    "yjs"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "standard --fix src/** && standard --fix test/** && tape test/*.js",
    "build": "browserify src/index.js -s Multihack -o static/js/multihack.js",
    "build-babel": "browserify src/index.js -s Multihack -o static/js/multihack.js -t [ babelify --presets [ es2015 ] ]",
    "watch": "watchify src/index.js -s Multihack -o static/js/multihack.js"
  },
  "author": "Chase Lee",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "cuid": "^1.3.8",
    "events": "^1.1.1",
    "get-browser-rtc": "^1.0.2",
    "getusermedia": "^2.0.1",
    "inherits": "^2.0.3",
    "list.js": "^1.5.0",
    "multihack-wire": "^2.0.1",
    "mustache": "^2.3.0",
    "p2p-graph": "^1.1.1",
    "readable-stream": "^2.2.9",
    "request": "^2.83.0",
    "safe-buffer": "^5.0.1",
    "simple-signal-client": "^1.2.6",
    "socket.io-client": "^2.0.2",
    "split.js": "^1.3.5",
    "stream-buffers": "^3.0.1",
    "stream-throttle": "^0.1.3",
    "y-array": "^10.0.6",
    "y-map": "^10.0.5",
    "y-memory": "^8.0.8",
    "y-richtext": "^9.0.8",
    "y-text": "^9.4.0",
    "y-webrtc": "^8.0.7",
    "y-websockets-client": "^8.0.16",
    "yjs": "^12.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rellat/rellat-web.git"
  },
  "bugs": {
    "url": "https://github.com/rellat/rellat-web/issues"
  },
  "homepage": "https://github.com/rellat/rellat-web#readme"
}
